public class Exercises 
{

	public static int threeAndFive()
	{
		int sum = 0;
		for (int i = 0; i < 1000; i++)
			if (i % 3 == 0 || i % 5 == 0)
				sum += i;
		return sum;
	}
	
	public static int evenFibSum()
	{
		int i = 1;
		return evenFibSumRec(i, 0); 
	}
	private static int evenFibSumRec(int i, int prev)
	{
		if (i > 4000000)
			return 0;
		if (i % 2 != 0)
			return i + evenFibSumRec((i + prev), i);
		else
			return evenFibSumRec((i + prev), i);
	}
	
	public static int largestPrimeFactor()
	{
		int currentLargestPrime = 0;
		long num = (long)(600851475143.0);
		long newNum = num;
		
		for (int i = 0; newNum == num; i++)
		{
			if (num % i == 0)
			{
				newNum = i;
				num = num/i;
				newNum = num;
			}
		}
		return num;
	}
	
	
	
	
}
