package Thapar_Avijit;

public class Exercises 
{
	public static int threeAndFive()
	{
		int sum = 0;
		for (int i=0; i<1000; i++)
		{
			if (i%5==0 || i%3==0)
				sum+=i;
		}
		return sum;
	}

	public static int fibHelp(int n)
	{
		if(n <= 1){
			return n;
		}
		int fibo = 1;
		int fiboPrev = 1;
		for(int i = 2; i < n; ++i){
			int temp = fibo;
			fibo += fiboPrev;
			fiboPrev = temp;
		}
		return fibo;
	}
	
	public static int evenFibSum()
	{
		int sum = 0;
		int i = 0;
		while (fibHelp(i) < 4000000)
		{
			if (fibHelp(i)%2 == 0)
				sum += fibHelp(i);
			i++;
		}
		return sum;
	}

	public static int largestPrimeFactor()
	{
		long i = 2;
		while (600851475143%i != 0)
		{
			i++;
		}
	}
}