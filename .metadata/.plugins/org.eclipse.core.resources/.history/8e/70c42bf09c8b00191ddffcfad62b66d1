	public static void main (String[] agrs)
	{
		System.out.println(threeAndFive());
	}
	
	public static int threeAndFive()
	{
		int num = 0;
		for (int i = 0; i<1000; i++)
		{
			if (i%3==0 || i%5==0)
				num+=i;
		}
		return num;
	}
