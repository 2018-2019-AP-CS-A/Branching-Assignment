public class Exercises 
{

	public static int threeAndFive()
	{
		int sum = 0;
		for (int i = 0; i < 1000; i++)
			if (i % 3 == 0 || i % 5 == 0)
				sum += i;
		return sum;
	}
	
	public static int evenFibSum()
	{
		int i = 1;
		
		return evenFibSumRec(i); 
	}
	private static int evenFibSumRec(int i)
	{
		if (i > 4000000)
			return 0;
		
	}
	
	
	
	
}
