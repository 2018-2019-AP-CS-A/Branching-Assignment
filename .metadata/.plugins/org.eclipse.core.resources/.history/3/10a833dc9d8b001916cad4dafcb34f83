package patel_rajat;

public class Exercises {
	public static void main(String... args) {
		System.out.println(threeAndFive());
	}
	
	public static int threeAndFive() {
		int s3 = (3 + 999) * 333/2;
		int s5_1 = (5 + 995) * 67/2;
		int s5_2 = (10 + 985) * 66/2;
		return s3+s5_1+s5_2;
	}
}
